//accordion stylesheet
#accordion {
    position: fixed; 
    bottom: 0;
    left: 0;
    background: #E0E3E1;
    width: 100%;
}
.panel-heading a:after{
    font-family: 'Glyphicons Halflings';
    content: '\e114';
    float: right;
    color:#e3e3e3;
}
.panel-heading a.collapsed:after{
    content: '\e080';
}

