/* CSS Document: 
   Overwrite some css properties in bootstrap 
   Define some css properties for this project
*/

body {
    font-family: tahoma, "Times New Roman", "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans",      Geneva, Verdana, sans-serif;
}

.small { font-size: 80%; }
ul li {
    list-style: none;
}
.clear {clear: both; }
a {
    text-decoration: none; 
}

a:hover{
    text-decoration:underline;
    background-color: transparent;
}

ul.navbar-nav li a:hover{
    color: #eea236 !important;
    background-color: transparent;
}
ul .navbar-nav li a:visited{
    color:#fff;
    background:inherit;
}
nav ul,nav li, .nav, .nav ul, .nav li, nav {
    margin:0;
    padding:0;
}


h2,.h2 {   
    font-size:20px;
    letter-spacing: -2px;
    color: #fff;
    //  padding-top: 35px;
    padding-top:0;
    padding-bottom:0px;
}

h3,.h3 { //for title use only 
         margin:10px 0 5px;
         letter-spacing: -1px;  //thin out letter spacing
         display:block;
         font-family: inherit;
         border-bottom:2px solid #336600;
         padding:15px 0px;
         font-size:20px;
         font-weight:500;
         color: #800000;
         white-space: nowrap;
}

h5, .h5 {
    margin:10px 0 5px;
    display:block;
    border-bottom:1px solid #F0F0F0;
    padding:15px 0px;
    font-size: 20px;
    font-weight:500;
    color: #800000;
    white-space: nowrap;
}

.container {
    width:100%;
}
//overwrite default carousel-control property

/*.carousel-control.left{ background-image: transparent important! }

.carousel-control.right{ background-image: transparent important! }*/

.background-none {background: transparent; }

@media (min-width: 1300px){
    .container {
        max-width:1300px;
    }
}

address {
    margin-left: 25%;
    display: table;
}

//change the default carousel height in carousel.css file
.carousel {
    margin-bottom: 0;
}

.carousel-indicators {
    position: absolute;
    bottom: 10px;
    left: 50%;
    width: 60%;
}

.carousel-indicators li {
    display: inline-block;
    bottom:20px;
    width: 20px;
    height: 20px;
    // background-color: #fff;
    background-color: transparent;
    border-radius: 20px;
}
.carousel-indicators .active {
    width: 22px;
    height: 22px;
    background-color: #800000;
}
.carousel-caption {
    bottom: 0px;
    text-decoration:none;
    padding-bottom:0px;
    text-align: center;
}

h4, .h4 { //for carousel's caption use only
          font-size: 18px;
          font-family: Didot, "Didot LT STD", "Hoefler Text", Georgia, Garamond, "Times New Roman", serif;	color:#044759;
          margin:10px 0 5px;
          display:block;
          color: #ffffff;
}

.img-circle {
    border-radius: 50%;
}
.logo {
    color:#660033;
    padding-top:1.5em;
    margin-left: 5%;
}
.logo img {
    display:inline-block;
    vertical-align: bottom;
    height: auto;
    width: 100%;
}
.logo a {
    color:#660033;
    display:block;
}

.logo a:hover { text-decoration:none; }

.thumb img {
    height: 50px; 
    width:25px;
}
header {
    padding-top:10px;
}
header {
    position:relative;
    z-index:1001;
}
.eyebrow .nav > li >a:hover, .eyebrow .nav > li > a:focus,
.eyebrow li.open>a {
    color:#fff;
}
.eyebrow .nav .open .caret,
.eyebrow .nav a:hover .caret {
    border-top-color: #ffffff;
    border-bottom-color: #ffffff;
}
.eyebrow .nav {
    margin-top: 35px;
}
.navbar-default {
    background-color: #800000;
}
.navbar-default {
    border-color: #717171;
    margin-bottom: 0px;
}

.navbar-default .navbar-nav>li>a {
    color:#fff;
}

.navbar-default .navbar-nav>.open>a, 
.navbar-default .navbar-nav>.open>a:hover, 
.navbar-default .navbar-default 
.navbar-nav>.open>a:focus{
    color:#ff0;
}



.navbar-custom {
    background-color: #800000;
}

.navbar-custom .dropdown-menu li:first-child>a {
    border-top:none;
}

.navbar {
    border-radius: 0px;
}

/*dropdown menu on hover display block*/
.dropdown-menu .sub-menu {
    left: 100%;
    position: absolute;
    top: 0;
    visibility: hidden;
    margin-top: -1px;
}

.dropdown-menu li:hover .sub-menu {
    visibility: visible;
}

.dropdown:hover .dropdown-menu {
    display: block;
}


.left-padding {
    padding-left: 3%;
}

.nowrap {
    white-space: nowrap;
}
.body {
    position:relative;
    z-index:5;
}
hr {
    -webkit-margin-before: 0.5em;
    -webkit-margin-after: 0.5em;
    -webkit-margin-start: auto;
    -webkit-margin-end: auto;
    margin: 15px auto;
    border: 1px solid #e3e3e3;
}

.body-col {
    padding-bottom:30px;
}

#calendar table {
    border: 1px solid #336600;
}

#calendar th{
    border-left: 1px solid #336600;
    border-right: 1px solid darkgreen;
    text-align: center;
    background-color: #c0c0c0;
    color: #fff;
}
#calendar tr {
    border-bottom: 1px solid darkgreen;
    font-weight: 200;
    text-align: center;
}

.title {
    font-size: 18px;
    font-family:inherit;
    letter-spacing: 0;
    padding: 15px 0;
    font-weight:400;
    color:black;
    white-space:nowrap;
}

#calendar td {
    border: 1px solid darkgreen;
}
#calendar td:first-child {
    font-weight: bold;
} 
footer {
    font-size:11px;
    border-bottom:25px solid #630000;//#032197;
    background-color:#800000;
    padding:5px 0;
    color:#fff;
    line-height:20px;
}

@media (max-width: 480px){
    .carousel-indicators {
        position: absolute;
        bottom: 35%;
        left: 50%;
        width: 60%;
    }

    .carousel-indicators li {
        display: inline-block;
        bottom:10px;
        width: 15px;
        height: 15px;
        background-color: #ffffff;
        border-radius: 15px;
    }
    .carousel-indicators .active {
        width: 16px;
        height: 16px;
        background-color: #800000;
    }

    h3, .h3 {
        font-size:20px;
    }
    header {
        padding-top:10px;
    }
    address {
        margin-left: 5%;
    }
}

.table tr td:first-child {
    text-align: left;
}

#improveTeaching img{
    margin: 10px auto 20px auto;
    padding: 0;
}

@media (max-width: 600px) {
    .logo img {
        padding-left: 0px;
        margin-left: -20px;
    }
}


@media (min-width: 768px){
    .navbar > .container .nav {
        margin-left:-15px;
    }
} 

@media (max-width: 767px) {
    .body-col {
        min-height:0 !important;
        padding-bottom:10px;
    }
    h3, .h3 {
        font-size:22px;
    }
    address {
        margin-left: 10%;
    }
}
@media (min-width: 768px){
    .navbar {
        margin-bottom: 0px;
    }   
} 

@media(min-width: 768px) and (max-width:1243px){
    h3, .h3 {
        white-space:nowrap;
    }
}

@media (min-width:845px) and (max-width:1230px){
    //TODO: address collapsible navbar 
}
@media screen and (max-width: 848px) {
    .carousel-indicators {
        bottom: 12%;
    }
}

@media (max-width:991px){
    .carousel-caption{
        display: none; 
    }
}

@media(max-width: 1000px){
    .container .col-sm-8 {
        padding-left:10%;
    }
}

